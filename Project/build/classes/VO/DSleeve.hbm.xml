<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="VO.DSleevesVO" table="Dsleeve_mst">

		<id name="sleeves_id" type="int" column="sleeves_id">
			<generator class="increment" />
		</id>

		<property name="sleeves_type">
			<column name="sleeves_type" />
		</property>

		<property name="sleeves_description">
			<column name="sleeves_description" />
		</property>

	</class>

</hibernate-mapping>
